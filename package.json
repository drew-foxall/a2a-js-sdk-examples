{
  "name": "a2a-js-sdk-examples",
  "version": "1.0.0",
  "description": "AI SDK + Hono examples for @drew-foxall/a2a-js-sdk",
  "author": "Drew Foxall",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/drew-foxall/a2a-js-sdk-examples.git"
  },
  "bugs": {
    "url": "https://github.com/drew-foxall/a2a-js-sdk-examples/issues"
  },
  "homepage": "https://github.com/drew-foxall/a2a-js-sdk-examples#readme",
  "scripts": {
    "install:all": "pnpm install",
    "agents:movie-agent": "cd examples/agents && pnpm agents:movie-agent",
    "agents:coder": "cd examples/agents && pnpm agents:coder",
    "agents:content-editor": "cd examples/agents && pnpm agents:content-editor",
    "agents:hello-world": "cd examples/agents && pnpm agents:hello-world",
    "agents:dice-agent": "cd examples/agents && pnpm agents:dice-agent",
    "agents:github-agent": "cd examples/agents && pnpm agents:github-agent",
    "agents:analytics-agent": "cd examples/agents && pnpm agents:analytics-agent",
    "agents:currency-agent": "cd examples/agents && pnpm agents:currency-agent",
    "agents:weather-agent": "cd examples/agents && pnpm agents:weather-agent",
    "agents:airbnb-agent": "cd examples/agents && pnpm agents:airbnb-agent",
    "agents:travel-planner": "cd examples/agents && pnpm agents:travel-planner",
    "build": "pnpm --filter @drew-foxall/a2a-ai-sdk-adapter build",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format .",
    "format:write": "biome format --write .",
    "check": "biome check ."
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.6",
    "tsx": "^4.19.2"
  },
  "pnpm": {
    "overrides": {
      "@ai-sdk/provider": "3.0.0-beta.16"
    },
    "onlyBuiltDependencies": [
      "canvas"
    ]
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "a2a",
    "agent",
    "ai-sdk",
    "hono",
    "examples",
    "vercel-ai-sdk",
    "llm",
    "openai",
    "anthropic",
    "google-ai"
  ]
}
