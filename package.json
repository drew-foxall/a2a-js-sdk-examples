{
  "name": "a2a-js-sdk-examples",
  "version": "1.0.0",
  "description": "AI SDK + Hono examples for @drew-foxall/a2a-js-sdk",
  "author": "Drew Foxall",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/drew-foxall/a2a-js-sdk-examples.git"
  },
  "bugs": {
    "url": "https://github.com/drew-foxall/a2a-js-sdk-examples/issues"
  },
  "homepage": "https://github.com/drew-foxall/a2a-js-sdk-examples#readme",
  "scripts": {
    "build": "turbo run build",
    "test": "turbo run test",
    "test:watch": "turbo run test:watch",
    "typecheck": "turbo run typecheck && tsc --noEmit",
    "typecheck:scripts": "tsc --noEmit",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "format": "turbo run format",
    "format:write": "turbo run format:write",
    "start-testing": "tsx scripts/start-testing.ts",
    "agent:movie": "pnpm --filter ./examples/agents agent:movie",
    "agent:coder": "pnpm --filter ./examples/agents agent:coder",
    "agent:content-editor": "pnpm --filter ./examples/agents agent:content-editor",
    "agent:hello-world": "pnpm --filter ./examples/agents agent:hello-world",
    "agent:dice": "pnpm --filter ./examples/agents agent:dice",
    "agent:github": "pnpm --filter ./examples/agents agent:github",
    "agent:analytics": "pnpm --filter ./examples/agents agent:analytics",
    "agent:currency": "pnpm --filter ./examples/agents agent:currency",
    "agent:weather": "pnpm --filter ./examples/agents agent:weather",
    "agent:airbnb": "pnpm --filter ./examples/agents agent:airbnb",
    "agent:planner": "pnpm --filter ./examples/agents agent:planner",
    "inspector": "tsx scripts/start-inspector.ts",
    "inspector:stop": "tsx scripts/stop-inspector.ts",
    "inspector:logs": "tsx scripts/inspector-logs.ts",
    "inspector:help": "echo 'ðŸ” A2A Inspector Commands:\n  pnpm inspector        - Start local inspector\n  pnpm inspector:stop   - Stop inspector\n  pnpm inspector:logs   - View logs\n  \nMore info: cat INSPECTOR_SETUP.md'"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.6",
		"@total-typescript/tsconfig": "1.0.4",
    "@types/node": "^22.0.0",
    "tsx": "^4.19.2",
    "turbo": "2.6.1",
    "typescript": "^5.8.0"
  },
  "pnpm": {
    "overrides": {
      "@ai-sdk/provider": "3.0.0-beta.16"
    },
    "onlyBuiltDependencies": [
      "canvas"
    ]
  },
  "packageManager": "pnpm@10.11.1",
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "a2a",
    "agent",
    "ai-sdk",
    "hono",
    "examples",
    "vercel-ai-sdk",
    "llm",
    "openai",
    "anthropic",
    "google-ai"
  ]
}
