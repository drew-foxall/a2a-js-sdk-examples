{
  "name": "a2a-js-sdk-examples",
  "version": "1.0.1",
  "description": "AI SDK + Hono examples for @drew-foxall/a2a-js-sdk",
  "author": "Drew Foxall",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/drew-foxall/a2a-js-sdk-examples.git"
  },
  "bugs": {
    "url": "https://github.com/drew-foxall/a2a-js-sdk-examples/issues"
  },
  "homepage": "https://github.com/drew-foxall/a2a-js-sdk-examples#readme",
  "scripts": {
    "build": "turbo run build",
    "test": "turbo run test",
    "test:watch": "turbo run test:watch",
    "typecheck": "turbo run typecheck",
    "typecheck:scripts": "tsc -p scripts/tsconfig.json --noEmit",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "format": "turbo run format",
    "format:write": "turbo run format:write",
    "dev:inspector": "tsx scripts/start-inspector.ts",
    "dev:test": "tsx scripts/dev-test.ts",
    "start-testing": "pnpm dev:test",
    "local:up": "tsx scripts/local-up.ts",
    "local:down": "tsx scripts/local-down.ts",
    "local:logs": "tsx scripts/local-logs.ts",
    "local:status": "tsx scripts/local-status.ts",
    "local:help": "echo 'ðŸ  Local Development Commands:\\n  pnpm local:up      - Start Redis + Upstash adapter\\n  pnpm local:down    - Stop local infrastructure\\n  pnpm local:logs    - View container logs\\n  pnpm local:status  - Check service status\\n\\nMore info: cat LOCAL_TESTING.md'",
    "agent:movie": "pnpm --filter ./examples/agents agent:movie",
    "agent:coder": "pnpm --filter ./examples/agents agent:coder",
    "agent:content-editor": "pnpm --filter ./examples/agents agent:content-editor",
    "agent:hello-world": "pnpm --filter ./examples/agents agent:hello-world",
    "agent:dice": "pnpm --filter ./examples/agents agent:dice",
    "agent:github": "pnpm --filter ./examples/agents agent:github",
    "agent:analytics": "pnpm --filter ./examples/agents agent:analytics",
    "agent:currency": "pnpm --filter ./examples/agents agent:currency",
    "agent:weather": "pnpm --filter ./examples/agents agent:weather",
    "agent:airbnb": "pnpm --filter ./examples/agents agent:airbnb",
    "agent:planner": "pnpm --filter ./examples/agents agent:planner",
    "inspector": "tsx scripts/start-inspector.ts",
    "inspector:stop": "tsx scripts/stop-inspector.ts",
    "inspector:logs": "tsx scripts/inspector-logs.ts",
    "inspector:help": "echo 'ðŸ” A2A Inspector Commands (Docker):\n  pnpm inspector        - Start inspector container\n  pnpm inspector:stop   - Stop inspector container\n  pnpm inspector:logs   - View container logs\n  \nMore info: cat INSPECTOR_SETUP.md'",
    "worker:hello-world": "pnpm --filter a2a-hello-world-worker dev",
    "worker:dice": "pnpm --filter a2a-dice-agent-worker dev",
    "worker:dice-durable": "pnpm --filter a2a-dice-agent-durable-worker dev",
    "worker:currency": "pnpm --filter a2a-currency-agent-worker dev",
    "worker:weather": "pnpm --filter a2a-weather-agent-worker dev",
    "worker:airbnb-mcp": "pnpm --filter airbnb-mcp-server-worker dev",
    "worker:airbnb": "pnpm --filter a2a-airbnb-agent-worker dev",
    "worker:planner": "pnpm --filter a2a-travel-planner-worker dev",
    "worker:deploy:hello-world": "pnpm --filter a2a-hello-world-worker run deploy",
    "worker:deploy:dice": "pnpm --filter a2a-dice-agent-worker run deploy",
    "worker:deploy:dice-durable": "pnpm --filter a2a-dice-agent-durable-worker run deploy",
    "worker:deploy:currency": "pnpm --filter a2a-currency-agent-worker run deploy",
    "worker:deploy:weather": "pnpm --filter a2a-weather-agent-worker run deploy",
    "worker:deploy:airbnb-mcp": "pnpm --filter airbnb-mcp-server-worker run deploy",
    "worker:deploy:airbnb": "pnpm --filter a2a-airbnb-agent-worker run deploy",
    "worker:deploy:planner": "pnpm --filter a2a-travel-planner-worker run deploy",
    "workers:deploy:all": "pnpm worker:deploy:hello-world && pnpm worker:deploy:dice && pnpm worker:deploy:currency && pnpm worker:deploy:weather && pnpm worker:deploy:airbnb-mcp && pnpm worker:deploy:airbnb && pnpm worker:deploy:planner"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.8",
    "@total-typescript/tsconfig": "1.0.4",
    "@types/node": "^24.10.1",
    "tsx": "^4.21.0",
    "turbo": "2.6.1",
    "typescript": "^5.9.3"
  },
  "pnpm": {
    "overrides": {
      "@ai-sdk/provider": "3.0.0-beta.16"
    },
    "onlyBuiltDependencies": [
      "canvas"
    ]
  },
  "packageManager": "pnpm@10.23.0",
  "engines": {
    "node": ">=24.0.0",
    "pnpm": ">=10.0.0"
  },
  "keywords": [
    "a2a",
    "agent",
    "ai-sdk",
    "hono",
    "examples",
    "vercel-ai-sdk",
    "llm",
    "openai",
    "anthropic",
    "google-ai"
  ]
}
