{
  "name": "@drew-foxall/a2a-ai-sdk-adapter",
  "version": "1.0.2",
  "description": "Unified adapter bridging Vercel AI SDK ToolLoopAgent with A2A protocol",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./durable": {
      "types": "./dist/durable-adapter.d.ts",
      "import": "./dist/durable-adapter.js"
    }
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts",
    "dist/**/*.d.ts.map",
    "dist/**/*.js.map",
    "!dist/**/*.test.*",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "typecheck": "tsc --noEmit",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format .",
    "format:write": "biome format --write .",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "a2a",
    "agent",
    "ai-sdk",
    "adapter",
    "vercel-ai-sdk",
    "hono",
    "typescript",
    "agent-to-agent",
    "multi-agent",
    "llm"
  ],
  "author": "Drew Foxall",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/drew-foxall/a2a-js-sdk-examples.git",
    "directory": "packages/a2a-ai-sdk-adapter"
  },
  "bugs": {
    "url": "https://github.com/drew-foxall/a2a-js-sdk-examples/issues"
  },
  "homepage": "https://github.com/drew-foxall/a2a-js-sdk-examples/tree/main/packages/a2a-ai-sdk-adapter#readme",
  "peerDependencies": {
    "@drew-foxall/a2a-js-sdk": "^0.4.2",
    "ai": "^6.0.0",
    "uuid": ">=11.0.0",
    "workflow": "4.0.1-beta.32"
  },
  "peerDependenciesMeta": {
    "workflow": {
      "optional": true
    }
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.8",
    "@drew-foxall/a2a-js-sdk": "^0.4.2",
    "@total-typescript/tsconfig": "1.0.4",
    "@types/node": "^24.10.1",
    "@types/uuid": "^11.0.0",
    "@vitest/ui": "^4.0.14",
    "ai": "6.0.0-beta.99",
    "typescript": "^5.9.3",
    "uuid": "^13.0.0",
    "vitest": "^4.0.14",
    "workflow": "4.0.1-beta.32",
    "zod": "^4.1.13"
  },
  "packageManager": "pnpm@10.23.0",
  "engines": {
    "node": ">=24.0.0",
    "pnpm": ">=10.0.0"
  }
}
